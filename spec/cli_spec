require 'cli'

describe Main do
  describe ".clearTerminal" do
    context "When invoked" do
      it "clears everything from the console" do
        expect(Main.clearTerminal).to output("").to_stdout
      end
    end
  end
end

describe Main do
  describe ".rspecTest" do
    context "when invoked" do
      it "returns 0" do
        expect(Main.rspecTest).to eql(0)
      end
    end
  end
end
